<% if @current_user.admin %>	
	<div class="Sign_Form">
	  <h1>Edit Account</h1>
	  <hr>
	  <%= form_for(@user_edit) do |f| %>
	  	<p> First Name:</br> <%= f.text_field :first_name%> </p>
	  	<p> Last Name:</br> <%= f.text_field :last_name%> </p>
	  	<p> Email Address:</br> <%= f.text_field :email%> </p>
	    <p> Username:</br> <%= f.text_field :username%> </p>
	    <p> Admin <%= f.check_box :admin%> </p>
	    <%= f.submit%>
	  <% end %>
	</div>
	<% else %>
		<center><p>You don't have permission</p></center>
	<% end %>